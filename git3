//comparision of 3 no number
#include<iostream>
using namespace std;
int main(){
	int a,b,c;
	cout<<"enter the value of a b c"<<endl;
	cin>>a>>b>>c;
	if(a>b && a>c){
		cout<<a<<"is a big number from "<<b<<" and "<<c<<endl;
	}
	else if(b>a && b>c){
		cout<<b<<"is a big number from "<<a<<" and "<<c<<endl;
	}
	else{
		cout<<c<<"is a big number from "<<a<<" and "<<b<<endl;
	}
	return 0;
}
